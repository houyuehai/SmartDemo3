<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--圆形进度条的属性-->
    <declare-styleable name="ProgressRegulator">
        <attr name="darkColor" format="color|reference"/>
        <attr name="lightColor" format="color|reference"/>
        <attr name="subTitleTextColor" format="color|reference"/>
        <attr name="sub2TitleTextColor" format="color|reference"/>
        <attr name="title1TextColor" format="color|reference"/>
        <!--圆环的宽度-->
        <attr name="roundWidth" format="integer"/>
        <!--圆环的底色-->
        <attr name="roundColor" format="color|reference"/>
        <!--圆环进度的颜色-->
        <attr name="roundProgressColor" format="color|reference"/>
        <!--边框与内容之间的空隙-->
        <attr name="padding" format="integer"/>
        <!--字体颜色-->
        <attr name="textColorLight" format="color|reference"/>
        <!--强调字体颜色-->
        <attr name="textColorDrak" format="color|reference"/>
        <!--字体颜色大小-->
        <attr name="lightTextSize" format="integer"/>
        <!--强调字体大小-->
        <attr name="darkTextSize" format="integer"/>



        <!--圆环进度最大值-->
        <attr name="max" format="integer"/>
        <!--圆环当前进度值-->
        <attr name="progress" format="integer"/>


        <!--开始的角度-->
        <attr name="startAngle" format="integer"/>

    </declare-styleable>
</resources>